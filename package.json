{
  "name": "grid-sight",
  "version": "0.1.0",
  "description": "A library for enriching HTML tables with data visualization and analysis tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:chart": "jest --testMatch='**/tests/**/*chart*.test.ts'",
    "test:a11y": "jest --testMatch='**/tests/**/*a11y*.test.ts'",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeepBlueCLtd/grid-sight.git"
  },
  "keywords": [
    "table",
    "visualization",
    "data",
    "analysis",
    "heatmap",
    "z-score"
  ],
  "author": "DeepBlue C Ltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeepBlueCLtd/grid-sight/issues"
  },
  "homepage": "https://github.com/DeepBlueCLtd/grid-sight#readme",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@storybook/html": "9.0.5",
    "@storybook/html-vite": "9.0.5",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "storybook": "9.0.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "uplot": "^1.6.24"
  }
}
