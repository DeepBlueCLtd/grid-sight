{
  "name": "va-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/copy-demo.js",
    "preview": "vite preview",
    "preview:demo": "vite preview --port 3000 --open grid-sight",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "vitest run --project=storybook",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "test:e2e": "vite build && playwright test tests/e2e",
    "test:e2e:ui": "vite build && playwright test --ui tests/e2e"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@storybook/addon-docs": "^9.0.6",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-vitest": "^9.0.9",
    "@storybook/html-vite": "^9.0.6",
    "@storybook/test": "^8.6.14",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/browser": "3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.28.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "fs-extra": "^11.3.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "storybook": "^9.0.6",
    "terser": "^5.42.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@types/react": "^19.1.8",
    "simple-statistics": "^7.8.8"
  }
}
